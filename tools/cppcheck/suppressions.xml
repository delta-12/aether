<?xml version="1.0"?>
<suppressions>

    <!-- Suppress missing includes -->
    <suppress>
        <id>missingInclude</id>
    </suppress>
    <suppress>
        <id>missingIncludeSystem</id>
    </suppress>

    <!-- Suppress unused functions -->
    <suppress>
        <id>unusedFunction</id>
    </suppress>

    <!-- Suppress variable can be declared as pointer to const false positive -->
    <suppress>
        <id>constVariablePointer</id>
        <fileName>*/src/hashmap.c</fileName>
        <lineNumber>119</lineNumber>
        <symbolName>value</symbolName>
    </suppress>

</suppressions>
